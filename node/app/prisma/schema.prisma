// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema


datasource db {
  provider = "mysql"
  url      = "mysql://root:root@mysql-container:3306/modulos"
}

generator client {
  provider = "prisma-client-js"
}

model cursos {
  cursoId   Int    @id
  cursoName String
}
